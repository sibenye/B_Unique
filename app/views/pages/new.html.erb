<div class="main_right">
<h2>New Page Creation</h2>
<div class="center">
<% form_for @page, :url => { :action => "create" } do |f| %>
  
  <div class="field">
    <%= f.label :Title %><br />
    <%= f.text_field :title, :value => @page.title, :maxlength => "40", :size => "25"%>
  </div>
  <div class="field">
    <%= f.label :Body %><br />
    <%= f.text_area :body, :size => "90x20" %>
  </div>
  <div class="field">
    <%= f.label :Page_Name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :Path_Name %><br />
    <%= f.text_field :path_name %>
  </div>
  
    
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


<%= link_to 'Back', dashboard_path %>
</div></div>
